{
	"name": "shopify-starter",
	"version": "1.0.0",
	"scripts": {
		"start": "npm-run-all --parallel webpack:watch theme:dev",
		"dev": "npm start",
		"build": "NODE_ENV=production yarn webpack:build",
		"clean": "rm -rf assets",
		"// WEBPACK //": "",
		"webpack:watch": "webpack watch",
		"webpack:build": "webpack build",
		"// THEME //": "",
		"theme:dev": "shopify theme dev -e def",
		"theme:push:all": "yarn build && shopify theme push",
		"theme:push:theme": "yarn build && shopify theme push -x 'templates/*.json' -x 'config/settings_schema.json'",
		"theme:pull:all": "shopify theme pull --nodelete",
		"theme:pull:data": "shopify theme pull --nodelete -x 'assets/*' -x 'layout/*' -x 'locales/*' -x 'sections/*' -x 'snippets/*' -x 'templates/*.liquid' -x 'config/settings_schema.json' "
	},
	"author": "https://hyp3rl.ink",
	"license": "MIT",
	"homepage": "https://github.com/mattgordils/hypershop",
	"devDependencies": {
		"babel-eslint": "^10.0.3",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.37.5",
		"prettier": "^2.8.8",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
		"@barba/core": "^2.10.3",
		"@barba/css": "^2.1.16",
		"autoprefixer": "^10.4.21",
		"color": "^4.2.3",
		"copy-webpack-plugin": "^10.2.4",
		"css-loader": "^6.11.0",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"dayjs": "^1.11.18",
		"embla-carousel": "^8.6.0",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-class-names": "^8.6.0",
		"embla-carousel-fade": "^8.6.0",
		"esbuild-loader": "^2.21.0",
		"lodash": "^4.17.21",
		"mini-css-extract-plugin": "^2.9.4",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.5",
		"postcss-loader": "^6.2.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"sass": "^1.93.2",
		"sass-loader": "^12.6.0",
		"tailwindcss": "^3.4.18",
		"webpack": "^5.102.0"
	}
}
